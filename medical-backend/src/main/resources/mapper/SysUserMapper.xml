<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pokfulamroad.admintemplate.system.mapper.SysUserMapper">


    <select id="selectByAccount" resultType="com.pokfulamroad.admintemplate.system.entity.domain.SysUser">
        select * from SYS_USER where account = #{account}
    </select>
</mapper>
